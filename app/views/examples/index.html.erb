<p id="notice"><%= notice %></p>

<h1>Example page</h1>

<table>
  <thead>
    <tr>
      <th>Grammar</th>
      <th>Language</th>
      <th>Sentence 1</th>
      <th>Sentence 2</th>
      <th>Clip name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @examples.each do |example| %>
      <tr>
        <td><%= example.grammar.description %></td>
        <td><%= example.language %></td>
        <td><%= example.sentence1 %></td>
        <td><%= example.sentence2 %></td>
        <td><%= example.clip_name %></td>
        <td><%= link_to 'Show', example %></td>
        <td><%= link_to 'Edit', edit_example_path(example) %></td>
        <td><%= link_to 'Destroy', example, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Go to new example',  new_example_path() %>